# Clawederous Environment Configuration
# Copy this file to .env and fill in your values

# AgentMail (recommended - no IMAP needed!)
AGENTMAIL_API_KEY=your_api_key_here
AGENTMAIL_INBOX=tawkie@agentmail.to

# Email (IMAP) - optional, AgentMail is preferred
IMAP_HOST=
IMAP_PORT=
IMAP_USER=
IMAP_PASS=

# Twitter (optional)
TWITTER_API_KEY=
TWITTER_API_SECRET=
TWITTER_ACCESS_TOKEN=
TWITTER_ACCESS_SECRET=

# Data Storage
DATA_DIR=./data

# Logging
LOG_LEVEL=info
